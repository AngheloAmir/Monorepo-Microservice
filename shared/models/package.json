{
  "name": "@monorepo/models",
  "version": "0.0.0",
  "main": "./index.ts",
  "types": "./index.ts",
  "exports": {
    ".": "./index.ts"
  },
  "scripts": {
    "lint": "eslint .",
    "db:generate": "prisma generate --schema=./prisma/schema.prisma",
